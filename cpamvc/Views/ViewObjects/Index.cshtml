<div class="container mb-5">
    <!-- First Card -->
    <div class="row">
        <div class="col-12 card card-3 pb-4 mt-5">
            <div class="row">
                <div class="col-6">
                    <div class="card text-center p-2 mb-4 card-3 title-card text-light"><span class="h4">View Objects</span></div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-1">


                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="h5" for="exampleFormControlSelect2">Select an object to view details</label>
                        <select multiple class="form-control" size="8" id="select-ratio">
                            <option>Object 1</option>
                            <option>Object 2</option>
                            <option>Object 3</option>
                            <option>Object 4</option>
                            <option>Object 5</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-1">


                </div>
                <div class="col-md-4 pr-5">
                    <!-- Title -->

                    <p class="h4">Details</p>
                    <table class="table">
                        <tr>
                            <th scope="row">Name</th>
                            <td>Object 1</td>
                        </tr>
                        <tr>
                            <th scope="row">Related terms</th>
                            <td>
                                <ul>
                                    <li>Debt</li>
                                    <li>Short term</li>
                                    <li>Long term</li>
                                </ul>
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">Attribute (is a)</th>
                            <td>
                                <ul>
                                    <li>is a object</li>
                                    <li>is another object</li>
                                </ul>
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">Association (is a part of)</th>
                            <td>
                                <ul>
                                    <li>is a object</li>
                                    <li>is another object</li>
                                </ul>
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">Casuality (caused by)</th>
                            <td>
                                <ul>
                                    <li>is a object</li>
                                    <li>is another object</li>
                                </ul>
                            </td>
                        </tr>
                    </table>
                </div>
                <!-- End First Column -->
                <!-- Second Column (Graph) -->
                <div class="col-md-1">


                </div>
            </div>
        </div>
